{
    "name": "friend-time",
    "version": "1.0.0",
    "author": "Kevin Novak",
    "description": "Discord bot for converting times between time zones.",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=12.14.0"
    },
    "main": "dist/start.js",
    "scripts": {
        "start": "tsc && node dist/start.js",
        "start-shard": "tsc && node dist/app.js",
        "start-pm2": "tsc && pm2 start process.json",
        "stop-pm2": "pm2 stop process.json",
        "delete-pm2": "pm2 delete process.json",
        "build": "tsc"
    },
    "dependencies": {
        "chrono-node": "1.4.6",
        "discord.js": "12.3.1",
        "moment-timezone": "0.5.31",
        "mysql": "2.18.1",
        "pm2": "^4.4.1",
        "request": "2.88.2",
        "request-promise": "4.2.6"
    },
    "devDependencies": {
        "@types/mysql": "2.15.15",
        "@types/node": "14.6.1",
        "@types/request": "2.48.5",
        "@types/request-promise": "4.1.46",
        "tslint": "^6.1.3",
        "typescript": "^4.0.2"
    }
}
